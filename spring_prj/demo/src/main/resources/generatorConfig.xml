<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  <!-- <classPathEntry location="C:\Program Files\MySQL\Connector.J 5.1\mysql-connector-java-5.1.33-bin.jar"/> -->
  // ↑ MySQLに接続するConnectorJのjarファイルを指定します
  <context id="context1" targetRuntime="MyBatis3Simple">
    <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
      connectionURL="jdbc:mysql://mysql/sample_schema"
      userId="dev_usr"
      password="dev_usr_pass">
    </jdbcConnection>

    <!-- <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver> -->

    <javaModelGenerator targetPackage="com.example.demo.MBGEntity" targetProject="\demo\src">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="com.example.demo.MBGSql"  targetProject="\demo\src">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.demo.MBGMapper"  targetProject="\demo\src">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <!-- <table schema="sample_schema" tableName="account" domainObjectName="xxx" > -->
    <table schema="sample_schema" tableName="account">
      <!-- <property name="useActualColumnNames" value="true"/>
      <generatedKey column="ID" sqlStatement="DB2" identity="true" />
      <columnOverride column="DATE_FIELD" property="startDate" />
      <ignoreColumn column="FRED" />
      <columnOverride column="LONG_VARCHAR_FIELD" jdbcType="VARCHAR" /> -->
    </table>
  </context>
</generatorConfiguration>